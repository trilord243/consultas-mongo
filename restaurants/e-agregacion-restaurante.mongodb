use("sample_restaurants");

//Identifique el top cinco de c√≥digos postales con la mayor cantidad de restaurantes italianos.

db.restaurants.aggregate([
  { $match: { cuisine: "Italian" } },
  { $group: { _id: "$address.zipcode", count: { $sum: 1 } } },
  { $sort: { count: -1 } },
  { $limit: 5 },
]);
